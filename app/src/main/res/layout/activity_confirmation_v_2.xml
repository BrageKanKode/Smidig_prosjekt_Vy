<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView10"
        android:layout_width="0dp"
        android:layout_height="225dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:rotation="-6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView_new_confirmation_from"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/sf_text"
                android:textColor="@color/colorBlack"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView_new_confirmation_to"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/sf_display"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/textView_new_confirmation_from"
                app:layout_constraintTop_toBottomOf="@+id/textView_new_confirmation_from" />

            <TextView
                android:id="@+id/textView_new_confirmation_string_departure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/sf_text"
                android:text="@string/avgang"
                android:textColor="@color/colorBlack"
                app:layout_constraintStart_toStartOf="@+id/imageView_line_inside_confirmation"
                app:layout_constraintTop_toBottomOf="@+id/imageView_line_inside_confirmation" />

            <TextView
                android:id="@+id/textView_new_confirmation_day"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="64dp"
                android:fontFamily="@font/sf_text"
                android:text="@string/mandag"
                android:textColor="@color/colorBlack"
                app:layout_constraintEnd_toEndOf="@+id/imageView_line_inside_confirmation"
                app:layout_constraintTop_toBottomOf="@+id/imageView_line_inside_confirmation" />

            <ImageView
                android:id="@+id/imageView_confirmation_checkbox_ticket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:contentDescription="@string/line"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_check_in_sircle" />

            <TextView
                android:id="@+id/textView_new_confirmation_departure_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/sf_display"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/textView_new_confirmation_string_departure"
                app:layout_constraintTop_toBottomOf="@+id/textView_new_confirmation_string_departure" />

            <TextView
                android:id="@+id/textView_confirmation_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/sf_text"
                android:text="@string/_25_mai"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/textView_new_confirmation_day"
                app:layout_constraintTop_toBottomOf="@+id/textView_new_confirmation_day" />

            <ImageView
                android:id="@+id/imageView_line_inside_confirmation"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="32dp"
                android:contentDescription="@string/line"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView_new_confirmation_to"
                app:srcCompat="@drawable/ic_line" />

            <ImageView
                android:id="@+id/imageView_confirmation_line_two"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:contentDescription="@string/line"
                app:layout_constraintEnd_toEndOf="@+id/imageView_line_inside_confirmation"
                app:layout_constraintStart_toStartOf="@+id/imageView_line_inside_confirmation"
                app:layout_constraintTop_toBottomOf="@+id/textView_confirmation_date"
                app:srcCompat="@drawable/ic_line" />

            <TextView
                android:id="@+id/textView_points_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/sf_display"
                android:text="@string/poeng"
                android:textColor="@color/colorBlack"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@+id/imageView_confirmation_line_two"
                app:layout_constraintTop_toBottomOf="@+id/imageView_confirmation_line_two" />

            <TextView
                android:id="@+id/textView_confirmation_points_new"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="32dp"
                android:fontFamily="@font/sf_text"
                android:textColor="#41927B"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/imageView_confirmation_line_two"
                app:layout_constraintTop_toBottomOf="@+id/imageView_confirmation_line_two" />

            <ImageView
                android:id="@+id/imageView_confirmation_leaf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:contentDescription="@string/leaf_image"
                app:layout_constraintBottom_toBottomOf="@+id/textView_confirmation_points_new"
                app:layout_constraintStart_toEndOf="@+id/textView_confirmation_points_new"
                app:layout_constraintTop_toTopOf="@+id/textView_confirmation_points_new"
                app:srcCompat="@drawable/ic_vector_leaf" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView_confirmation_headline_title_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/sf_display"
        android:text="@string/billetten_er_lastet_ned"
        android:textColor="@color/colorBlack"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/cardView10"
        app:layout_constraintStart_toStartOf="@+id/cardView10"
        app:layout_constraintTop_toBottomOf="@+id/cardView10" />

    <TextView
        android:id="@+id/textView_confirmation_headline_title_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="86dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/sf_display"
        android:text="@string/god_tur"
        android:textColor="@color/colorBlack"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView_confirmation_headline_title_one"
        app:layout_constraintTop_toBottomOf="@+id/textView_confirmation_headline_title_one" />

    <Button
        android:id="@+id/button_to_points_overview"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#41927B"
        android:fontFamily="@font/sf_display"
        android:text="@string/til_billettoversikt"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_confirmation_headline_title_two" />

    <Button
        android:id="@+id/button_to_miljo_points"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sf_display"
        android:text="@string/til_milj_poeng"
        android:textColor="@color/colorBlack"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_to_points_overview" />

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView_new_confirmation_retur_string"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:fontFamily="@font/sf_display"
                android:text="@string/buy_returbillett"
                android:textColor="@color/colorBlack"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/button_no_clue"
                android:layout_width="76dp"
                android:layout_height="39dp"
                android:layout_marginEnd="16dp"
                android:backgroundTint="#CBEAE4"
                android:fontFamily="@font/sf_display"
                android:text="@string/velg"
                android:textColor="#3A846F"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>