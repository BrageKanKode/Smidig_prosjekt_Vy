<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:layout_editor_absoluteX="145dp"
    tools:layout_editor_absoluteY="125dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginRight="20dp"
    android:layout_marginLeft="20dp"
    android:background="@color/white">

    <TextView
        android:id="@+id/textView_explanation_travel_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/sfprodisplay_bold"
        android:text="@string/reise_med_poengene"
        android:textColor="@color/mainBlack"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_explanation_travel_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/sfprotext_regular"
        android:textSize="16sp"
        android:text="@string/explanation_travel_dialog"
        android:textColor="@color/mainBlack"
        app:layout_constraintStart_toStartOf="@+id/textView_explanation_travel_title"
        app:layout_constraintTop_toBottomOf="@+id/textView_explanation_travel_title" />

    <TextView
        android:id="@+id/textViewtextView_explanation_travel_good_to_know_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/sfprodisplay_bold"
        android:text="@string/godt_vite"
        android:textColor="@color/mainBlack"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView_explanation_travel_dialog"
        app:layout_constraintTop_toBottomOf="@+id/textView_explanation_travel_dialog" />

    <ImageView
        android:id="@+id/imageView_good_to_know_arrow_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/closing_bracket"
        app:layout_constraintStart_toStartOf="@+id/textViewtextView_explanation_travel_good_to_know_title"
        app:layout_constraintTop_toBottomOf="@+id/textViewtextView_explanation_travel_good_to_know_title"
        app:srcCompat="@drawable/ic_chevron_right_24px" />

    <TextView
        android:id="@+id/textViewtextView_explanation_travel_good_to_know_dialog_one"
        android:layout_width="0dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/sfprotext_regular"
        android:textColor="@color/mainBlack"
        android:layout_marginEnd="32dp"
        android:text="@string/good_to_know_dialog_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView_good_to_know_arrow_one"
        app:layout_constraintTop_toTopOf="@+id/imageView_good_to_know_arrow_one" />

    <ImageView
        android:id="@+id/imageView_good_to_know_arrow_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/closing_bracket"
        app:layout_constraintStart_toStartOf="@+id/imageView_good_to_know_arrow_one"
        app:layout_constraintTop_toBottomOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_one"
        app:srcCompat="@drawable/ic_chevron_right_24px" />

    <TextView
        android:id="@+id/textViewtextView_explanation_travel_good_to_know_dialog_two"
        android:layout_width="290dp"
        android:layout_height="25dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/sfprotext_regular"
        android:text="@string/good_to_know_dialog_two"
        android:textColor="@color/mainBlack"
        app:layout_constraintEnd_toEndOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_one"
        app:layout_constraintStart_toEndOf="@+id/imageView_good_to_know_arrow_two"
        app:layout_constraintTop_toTopOf="@+id/imageView_good_to_know_arrow_two" />

    <ImageView
        android:id="@+id/imageView_good_to_know_arrow_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/closing_bracket"
        app:layout_constraintStart_toStartOf="@+id/imageView_good_to_know_arrow_two"
        app:layout_constraintTop_toBottomOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_two"
        app:srcCompat="@drawable/ic_chevron_right_24px" />

    <TextView
        android:id="@+id/textViewtextView_explanation_travel_good_to_know_dialog_three"
        android:layout_width="297dp"
        android:layout_height="70dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/sfprotext_regular"
        android:text="@string/good_to_know_dialog_three"
        android:textColor="@color/mainBlack"
        app:layout_constraintEnd_toEndOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_two"
        app:layout_constraintStart_toEndOf="@+id/imageView_good_to_know_arrow_three"
        app:layout_constraintTop_toTopOf="@+id/imageView_good_to_know_arrow_three" />

    <ImageView
        android:id="@+id/imageView_good_to_know_arrow_four"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/closing_bracket"
        app:layout_constraintStart_toStartOf="@+id/imageView_good_to_know_arrow_three"
        app:layout_constraintTop_toBottomOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_three"
        app:srcCompat="@drawable/ic_chevron_right_24px" />

    <TextView
        android:id="@+id/textViewtextView_explanation_travel_good_to_know_dialog_four"
        android:layout_width="0dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/sfprotext_regular"
        android:text="@string/good_to_know_dialog_four"
        android:textColor="@color/mainBlack"
        app:layout_constraintEnd_toEndOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_three"
        app:layout_constraintStart_toEndOf="@+id/imageView_good_to_know_arrow_four"
        app:layout_constraintTop_toTopOf="@+id/imageView_good_to_know_arrow_four" />

    <ImageView
        android:id="@+id/imageView_good_to_know_arrow_five"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/closing_bracket"
        app:layout_constraintStart_toStartOf="@+id/imageView_good_to_know_arrow_four"
        app:layout_constraintTop_toBottomOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_four"
        app:srcCompat="@drawable/ic_chevron_right_24px" />

    <TextView
        android:id="@+id/textViewtextView_explanation_travel_good_to_know_dialog_five"
        android:layout_width="0dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/sfprotext_regular"
        android:text="@string/good_to_know_dialog_five"
        android:textColor="@color/mainBlack"
        app:layout_constraintEnd_toEndOf="@+id/textViewtextView_explanation_travel_good_to_know_dialog_four"
        app:layout_constraintStart_toEndOf="@+id/imageView_good_to_know_arrow_five"
        app:layout_constraintTop_toTopOf="@+id/imageView_good_to_know_arrow_five" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>