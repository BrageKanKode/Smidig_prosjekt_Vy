<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textview_starbucks_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/textview_price_starbucks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/textview_starbucks_dialog"
        app:layout_constraintStart_toStartOf="@+id/textview_starbucks_dialog"
        app:layout_constraintTop_toBottomOf="@+id/textview_starbucks_dialog"
        />


    <TextView
        android:id="@+id/textview_currency_starbucks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/textview_price_starbucks"
        app:layout_constraintStart_toStartOf="@+id/textview_price_starbucks"
        app:layout_constraintTop_toBottomOf="@+id/textview_price_starbucks"

        />


    <Button
        android:id="@+id/button_buy_starbucks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="KjÃ¸p"
        app:layout_constraintEnd_toEndOf="@+id/textview_currency_starbucks"
        app:layout_constraintStart_toStartOf="@+id/textview_currency_starbucks"
        app:layout_constraintTop_toBottomOf="@+id/textview_currency_starbucks"
        />

    <Button
        android:id="@+id/button_cancel_starbucks_payment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Avbryt"
        app:layout_constraintEnd_toEndOf="@+id/button_buy_starbucks"
        app:layout_constraintStart_toStartOf="@+id/button_buy_starbucks"
        app:layout_constraintTop_toBottomOf="@+id/button_buy_starbucks"
        />

</androidx.constraintlayout.widget.ConstraintLayout>